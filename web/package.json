{
  "name": "typing-mentor",
  "private": true,
  "scripts": {
    "clean": "npx rimraf .next",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint -- --fix",
    "format": "prettier --list-different \"**/*.{js,jsx,ts,tsx,json,htm,html,yml,yaml}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,htm,html,yml,yaml}\"",
    "typecheck": "tsc --project . --noEmit",
    "pr-preflight": "npm run typecheck && npm run format:fix && npm run lint:fix"
  },
  "dependencies": {
    "@next/bundle-analyzer": "12.0.7",
    "@reduxjs/toolkit": "1.6.2",
    "@types/js-cookie": "3.0.1",
    "cookie": "0.4.1",
    "firebase": "9.6.1",
    "firebase-admin": "10.0.0",
    "js-cookie": "3.0.1",
    "next": "12.0.2",
    "next-absolute-url": "1.2.2",
    "next-firebase-auth": "1.0.0-canary.2",
    "next-sanity": "0.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-firebaseui": "6.0.0",
    "react-redux": "7.2.6",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@types/cookie": "0.4.1",
    "@types/node": "16.11.6",
    "@types/react": "17.0.34",
    "@types/redux-logger": "3.0.9",
    "@types/styled-components": "5.1.15",
    "@welldone-software/why-did-you-render": "6.2.3",
    "babel-plugin-styled-components": "2.0.1",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "7.0.0",
    "prettier": "2.5.1",
    "redux-logger": "3.0.6",
    "sanity-codegen": "0.9.8",
    "styled-components": "5.3.3",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": "14.15.1"
  }
}
